<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE configuration PUBLIC
"-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<typeAlias type="com.kh.fitnessground.user.model.vo.User" alias="User" />
		<typeAlias type="com.kh.fitnessground.user.model.vo.UserSchedule" alias="UserSchedule" />
		<typeAlias type="com.kh.fitnessground.workout.health.model.vo.Health" alias="Health"/>
		<typeAlias type="com.kh.fitnessground.workout.yoga.model.vo.Yoga" alias="Yoga"/>
	</typeAliases>
	
	<!-- 사용할 매퍼 파일 등록 : 필수 -->
	<!-- 주의 : 등록된 매퍼 파일중에 오류가 있으면, 다른 매퍼에도 영향을 줌 -->
	<mappers>
		<mapper resource="mappers/user-mapper.xml" />
		<mapper resource="mappers/health-mapper.xml"/>
		<mapper resource="mappers/yoga-mapper.xml"/> 
	</mappers>
</configuration>















